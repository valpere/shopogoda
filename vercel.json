{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.go",
      "use": "@vercel/go"
    },
    {
      "src": "api/health.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    }
  ],
  "env": {
    "GO111MODULE": "on",
    "CGO_ENABLED": "0"
  }
}
